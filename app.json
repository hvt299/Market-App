{
  "expo": {
    "name": "Market App",
    "slug": "MarketApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-app.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-app.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-app.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.huathai.MarketApp"
    },
    "web": {
      "favicon": "./assets/logo-app.png"
    },
    "extra": {
      "eas": {
        "projectId": "98b8b276-6498-4c85-a5cc-9e6a28f85c31"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
